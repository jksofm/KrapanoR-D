	<!-- startup action - here the first pano/scene will be loaded -->
	<action name="startup" autorun="onstart">
	    if(startscene === null, copy(startscene,scene[0].name));
	    loadscene(get(startscene), null, MERGE);
	    delayedcall(0, skin_hideskin() );
	</action>

	
